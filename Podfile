  platform :tvos, '9.0'
  use_frameworks!

  # Pods for tvOS App

  target 'ProjectTemplate tvOS AppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ProjectTemplate tvOS AppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
