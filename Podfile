target 'ProjectTemplate' do
  platform :ios, '9.0'
  use_frameworks!

  # Pods for iOS App

  target 'ProjectTemplateTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ProjectTemplateUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

